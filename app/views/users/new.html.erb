<h2>Sign Up</h2>
<% if @errors %>
  <% @errors.each do |error| %>
  <p><%= error %></p>
  <% end %>
<% end %>
<%= form_for @user do |f| %>
 <p>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </p>
   <p>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
