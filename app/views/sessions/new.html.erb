<h2>Login</h2>


<form action='/sessions' method='POST'>
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <label>Email:</label>
   <input type='text' name='email'>
  <label>Password:</label>
    <input type='password' name='password'>
  <input type='submit' value='Login'>
</form>





